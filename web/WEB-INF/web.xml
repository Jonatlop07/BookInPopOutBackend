        <servlet-class>com.jonathan.RegUsuarioNormal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CancelarCita</servlet-name>
        <servlet-class>com.jonathan.CancelarCita</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreacionCola</servlet-name>
        <servlet-class>com.jonathan.CreacionCola</servlet-class>
